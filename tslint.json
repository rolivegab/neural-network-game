{
	"extends": "tslint:recommended",
	"rules": {
		"indent": [
			true,
			"tabs"
		],
		"quotemark": false,
		"no-console": false,
		"member-access": false,
		"max-line-length": false,
		"object-literal-sort-keys": false,
		"arrow-parens": false,
		"no-unused-expression": {
			"options": "allow-tagged-template"
		},
		"trailing-comma": [
			true,
			{
				"multiline": {
					"objects": "always",
					"arrays": "always",
					"functions": "never",
					"typeLiterals": "ignore"
				}
			}
		],
		"semicolon": [true, "never"],
		"variable-name": false
	}
}